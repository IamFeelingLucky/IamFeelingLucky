{
	"manifest_version": 2,

	"name": "IamFeelingLucky v2",
	"description": "directory.io helper",
	"version": "0.1",

	"permissions": [
		"http://blockchain.info/"
	],

	"content_scripts": [
		{
			"matches": ["http://directory.io/*?v2"],
			"css": ["helper.css"],
			"js": ["helper.js"],
			"run_at": "document_end"
		}
	]
}